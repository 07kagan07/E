@model IEnumerable<And.Eticaret.Core.Model1.Entity.UserAdress>

@{
    ViewBag.Title = "AdressList";
}



<p>
    @Html.ActionLink("Yeni Adres Ekle", "CreateUserAdress")
</p>
<table class="table">
    <tr>
        
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        
        <th>
            @Html.DisplayNameFor(model => model.Adress)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.Adress)
        </td>
        
        <td>
            @Html.ActionLink("Seç", "CreateOrder", new { id=item.ID })
        </td>
    </tr>
}

</table>
